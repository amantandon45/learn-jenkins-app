{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition",
    "containerDefinitions": [
        {
            "name": "LearnJenkinsApp",
            "image": "896607973004.dkr.ecr.us-east-1.amazonaws.com/learnjenkinsapp:1.0.13",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http",
                    "name": "nginx-80-tip"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::896607973004:role/ecsTaskExecutionRole"
}
